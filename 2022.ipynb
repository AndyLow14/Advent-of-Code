    "file = open(\"old_text.txt\", \"r\")\n",
    "\n",
    "max_cal = 0\n",
    "curr_cal = 0\n",
    "  \n",
    "for line in file:\n",
    "   if line in ['\\n', '\\r\\n']:\n",
    "      if curr_cal > max_cal:\n",
    "        max_cal = curr_cal\n",
    "      curr_cal = 0\n",
    "   else:\n",
    "    curr_cal += int(line)   \n",
    "   \n",
    "\n",
    "print(max_cal)"
   ]
  }
